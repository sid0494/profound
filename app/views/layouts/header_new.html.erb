<!DOCTYPE html>
<html>
  <head>
    <meta charset="UTF-8">
    <title>PROfound</title>
    <meta content='width=device-width, initial-scale=1, maximum-scale=1, user-scalable=no' name='viewport'>
    <!-- Bootstrap 3.3.2 -->
    <!-- <link href="bootstrap/css/bootstrap.min.css" rel="stylesheet" type="text/css" />     -->
    <%= stylesheet_link_tag "bootstrap.min" %>
    <!-- FontAwesome 4.3.0 -->
    <!-- <link href="https://maxcdn.bootstrapcdn.com/font-awesome/4.3.0/css/font-awesome.min.css" rel="stylesheet" type="text/css" /> -->
    <%= stylesheet_link_tag "font-awesome.min" %>
    <!-- Ionicons 2.0.0 -->
    <link href="http://code.ionicframework.com/ionicons/2.0.0/css/ionicons.min.css" rel="stylesheet" type="text/css" />
    <link href="//cdnjs.cloudflare.com/ajax/libs/select2/4.0.0-rc.2/css/select2.min.css" rel="stylesheet" />     
    <!-- Theme style -->
    <!-- <link href="dist/css/AdminLTE.min.css" rel="stylesheet" type="text/css" /> -->
    <%= stylesheet_link_tag "AdminLTE.min" %>
    <!-- AdminLTE Skins. Choose a skin from the css/skins 
         folder instead of downloading all of them to reduce the load. -->
    <!-- <link href="dist/css/skins/_all-skins.min.css" rel="stylesheet" type="text/css" /> -->
    <%= stylesheet_link_tag "skin-blue.min" %>
    
    <!-- iCheck -->
    <!-- <link href="plugins/iCheck/flat/blue.css" rel="stylesheet" type="text/css" /> -->
    <%= stylesheet_link_tag "green" %>
    <!-- Morris chart -->
    <!-- <link href="plugins/morris/morris.css" rel="stylesheet" type="text/css" /> -->
    <!-- jvectormap -->
    <!-- <link href="plugins/jvectormap/jquery-jvectormap-1.2.2.css" rel="stylesheet" type="text/css" /> -->
    <%= stylesheet_link_tag "jquery-jvectormap-1.2.2" %>
    <!-- Date Picker -->
    <!-- <link href="plugins/datepicker/datepicker3.css" rel="stylesheet" type="text/css" /> -->
    <!-- Daterange picker -->
    <!-- <link href="plugins/daterangepicker/daterangepicker-bs3.css" rel="stylesheet" type="text/css" /> -->
    <!-- bootstrap wysihtml5 - text editor -->
    <link href="plugins/bootstrap-wysihtml5/bootstrap3-wysihtml5.min.css" rel="stylesheet" type="text/css" />
    <%= stylesheet_link_tag "bootstrap3-wysihtml5.min" %>
    <!-- HTML5 Shim and Respond.js IE8 support of HTML5 elements and media queries -->
    <!-- WARNING: Respond.js doesn't work if you view the page via file:// -->
    <!--[if lt IE 9]>
        <script src="https://oss.maxcdn.com/libs/html5shiv/3.7.0/html5shiv.js"></script>
        <script src="https://oss.maxcdn.com/libs/respond.js/1.3.0/respond.min.js"></script>
    <![endif]-->
  </head>
  <body class="skin-blue">
    <div class="wrapper">
      
      <header class="main-header">
        <!-- Logo -->
        <%= link_to "<b>PRO</b>found".html_safe, root_path, :class => 'logo' %>
        <!-- <a href="index2.html" class="logo"><b>PRO</b>found</a> -->
        <!-- Header Navbar: style can be found in header.less -->
        <nav class="navbar navbar-static-top" role="navigation">
          <!-- Sidebar toggle button-->
          <a href="#" class="sidebar-toggle" data-toggle="offcanvas" role="button">
            <span class="sr-only">Toggle navigation</span>
          </a>
          <div class="navbar-custom-menu">
            <ul class="nav navbar-nav">
              <!-- Messages: style can be found in dropdown.less-->
              <li class="dropdown notifications-menu">
                <%= link_to "<i class='fa fa-envelope-o'></i> Messages".html_safe, messages_show_conversations_path %>
              </li>
              <!-- Notifications: style can be found in dropdown.less -->
              <li class="dropdown notifications-menu">
                <%= link_to "<i class='fa fa-bell-o'></i> Notifications".html_safe, dashboard_show_notifications_path %>
              </li>
              <!-- User Account: style can be found in dropdown.less -->
              <li class="dropdown user user-menu">
                <a href="#" class="dropdown-toggle" data-toggle="dropdown">
                <i class="fa fa-user"></i>
                  <span class="hidden-xs"><%= current_user.first_name + " " + current_user.last_name %></span>
                </a>
                <ul class="dropdown-menu">
                  <!-- User image -->
                  <li class="user-header">
                    <center><i class="fa fa-user fa-5x"></i></center>
                    <p>
                       <%= current_user.username %>
                      <small><%= current_user.first_name + " " + current_user.last_name %></small>
                    </p>
                  </li>
                  <!-- Menu Body -->
                  <li class="user-body">
                    <div class="col-xs-6 text-center">
                      <a href="#">Followers (<%= current_user.followers.count %>)</a>
                    </div>
                    <div class="col-xs-6 text-center">
                      <a href="#">Following (<%= current_user.followings.count %>)</a>
                    </div>
                  </li>
                  <!-- Menu Footer-->
                  <li class="user-footer">
                    <div class="pull-left">
                    <%= link_to "Profile", dashboard_show_profile_path(:id => current_user.id), :method => :get, :class => 'btn btn-default btn-flat'%>
                    </div>
                    <div class="pull-right">
                      <%= button_to "Sign out", destroy_user_session_path, :method => :delete, :class => 'btn btn-default btn-flat'%>
                    </div>
                  </li>
                </ul>
              </li>
            </ul>
          </div>
        </nav>
      </header>
      <!-- Left side column. contains the logo and sidebar -->
      <aside class="main-sidebar">
        <!-- sidebar: style can be found in sidebar.less -->
        <section class="sidebar">
          <!-- Sidebar user panel -->
          <div class="user-panel">
            <div class="pull-left info">
              <p><i class="fa fa-user"></i>  <%= " " + current_user.first_name + " " + current_user.last_name %></p>

              <a href="#"><i class="fa fa-circle text-success"></i> Online</a>
            </div>
          </div>
          <ul class="sidebar-menu">
            <li class="header">MAIN NAVIGATION</li>
            <li>
                <%= link_to "<i class='fa fa-fw fa-home'></i> Home".html_safe, "/dashboard/home" %>
            </li>
            <li>
            <li>
                <%= link_to "<i class='fa fa-fw fa-graduation-cap'></i> Projects".html_safe, '/projects/index' %>
            </li>
            </li>
            <li>
                <%= link_to"<i class='fa fa-fw fa-suitcase'></i> Discussion Forum".html_safe, '/discussions/index' %>
            </li>
            <li>
                <%= link_to "<i class='fa fa-fw fa-paste'></i> Learning-Teaching".html_safe, '/learning_topics/index' %>
            </li>
            <li>
                <%= link_to "<i class='fa fa-fw fa-hand-o-up'></i> Help".html_safe, "/dashboard/help" %>
            </li>
            <li>
                <%= link_to "<i class='fa fa-fw fa-question'></i> FAQs".html_safe, "/dashboard/faqs" %>
            </li>
          </ul>
        </section>
        <!-- /.sidebar -->
      </aside>

      <!-- Content Wrapper. Contains page content -->
      <div class="content-wrapper">

        <%= yield %>

      </div><!-- /.content-wrapper -->
      <footer class="main-footer">
        <div class="pull-right hidden-xs">
          <b>Version</b> alpha
        </div>
        <strong>PROfound 2015 </strong>
        &#149;
        <%= link_to " <i class='fa fa-fw fa-book'></i> About Us".html_safe, "/dashboard/about_us" %>
        &#149;
        <%= link_to " <i class='fa fa-fw fa-phone'></i> Contact Us".html_safe, "/dashboard/contact_us" %>
      </footer>
    </div><!-- ./wrapper -->

    <!-- jQuery 2.1.3 
    <script src="plugins/jQuery/jQuery-2.1.3.min.js"></script> -->
    <%= javascript_include_tag "jQuery-2.1.3.min" %>
    <!-- jQuery UI 1.11.2 -->
    <script src="http://code.jquery.com/ui/1.11.2/jquery-ui.min.js" type="text/javascript"></script>
    <script src="//cdnjs.cloudflare.com/ajax/libs/select2/4.0.0-rc.2/js/select2.min.js"></script>
    <script type="text/javascript">
      $('select').select2({
        tags: true
      });
    </script>
    <script type="text/javascript">
      $(".js-example-basic-multiple").select2({
        maximumSelectionLength: 1
      });
    </script>
    <!-- Resolve conflict in jQuery UI tooltip with Bootstrap tooltip -->
    <script>
      $.widget.bridge('uibutton', $.ui.button);
    </script>
    <!-- Bootstrap 3.3.2 JS -->
    <%= javascript_include_tag "bootstrap" %>
    <!--<script src="bootstrap/js/bootstrap.min.js" type="text/javascript"></script> -->   
    <!-- Morris.js charts -->
    <!--<script src="http://cdnjs.cloudflare.com/ajax/libs/raphael/2.1.0/raphael-min.js"></script>
    <script src="plugins/morris/morris.min.js" type="text/javascript"></script> -->
    <!-- Sparkline 
    <script src="plugins/sparkline/jquery.sparkline.min.js" type="text/javascript"></script>-->
    <%= javascript_include_tag "jquery.sparkline.min" %>
    <!-- jvectormap 
    <script src="plugins/jvectormap/jquery-jvectormap-1.2.2.min.js" type="text/javascript"></script>
    <script src="plugins/jvectormap/jquery-jvectormap-world-mill-en.js" type="text/javascript"></script>-->
    <%= javascript_include_tag "jquery-jvectormap-1.2.2.min" %>
    <%= javascript_include_tag "jquery-jvectormap-world-mill-en" %>
    <!-- jQuery Knob Chart 
    <script src="plugins/knob/jquery.knob.js" type="text/javascript"></script>-->
    <%= javascript_include_tag "jquery.knob" %>
    <!-- daterangepicker 
    <script src="plugins/daterangepicker/daterangepicker.js" type="text/javascript"></script>-->
    <!-- datepicker 
    <script src="plugins/datepicker/bootstrap-datepicker.js" type="text/javascript"></script>-->
    <!-- Bootstrap WYSIHTML5 
    <script src="plugins/bootstrap-wysihtml5/bootstrap3-wysihtml5.all.min.js" type="text/javascript"></script>-->
    <%= javascript_include_tag "bootstrap3-wysihtml5.all.min" %>
    <!-- iCheck 
    <script src="plugins/iCheck/icheck.min.js" type="text/javascript"></script>-->
    <%= javascript_include_tag "icheck.min" %>
    <!-- Slimscroll 
    <script src="plugins/slimScroll/jquery.slimscroll.min.js" type="text/javascript"></script>-->
    <%= javascript_include_tag "jquery.slimscroll.min" %>
    <!-- FastClick 
    <script src='plugins/fastclick/fastclick.min.js'></script>-->
    <%= javascript_include_tag "fastclick.min" %>
    <!-- AdminLTE App 
    <script src="dist/js/app.min.js" type="text/javascript"></script>-->
    <%= javascript_include_tag "app.min" %>

    <!-- AdminLTE dashboard demo (This is only for demo purposes) 
    <script src="dist/js/pages/dashboard.js" type="text/javascript"></script>-->
    <%= javascript_include_tag "dashboard" %>

    <!-- AdminLTE for demo purposes 
    <script src="dist/js/demo.js" type="text/javascript"></script>-->
    <%= javascript_include_tag "demo" %>
  </body>
</html>