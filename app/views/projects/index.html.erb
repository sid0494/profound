<section class="content-header">
  <h1>
    <b><i class="fa fa-fw fa-graduation-cap"></i> Projects </b><small> <i class="fa fa-circle text-success"></i> Completed <i class="fa fa-circle text-primary"></i> Ongoing <i class="fa fa-circle text-danger"></i> Yet to start </small><%= link_to "Create Project", projects_new_path, :class => "btn btn-primary pull-right" %> 
  </h1>
</section>
<hr>

<section class="content">
	<br>
	<%= form_tag projects_search_path, :method => 'get' do %>
	    <%= select_tag "search", options_for_select(@search_objects.collect{ |u| [u[:name], u] }), multiple: true, maximumSelectionLength: 1, class: "js-example-basic-multiple", :style => "width: 92%" %>
	    <%= submit_tag "Search", :name => nil, class: 'btn btn-warning btn-flat' %>
	<% end %><br>
  <% @projects.each do |project| %>
  <%=  render partial: "project", locals: {project: project}   %>
  <% end %>
</section>

<div class="clearfix"></div>